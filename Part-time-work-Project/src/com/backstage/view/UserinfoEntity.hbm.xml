<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.backstage.view.UserinfoEntity" table="userinfo" schema="demo">
        <id name="number">
            <column name="number" sql-type="int(11)"/>
        </id>
        <property name="sex">
            <column name="sex" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phonenumber">
            <column name="phonenumber" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="school">
            <column name="school" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>